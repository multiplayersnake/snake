{
  "name": "snake",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "csr:dev": "webpack serve --mode=development --open --hot --config=./webpack.csr.config.js",
    "csr:build": "webpack --mode=production --config=./webpack.csr.config.js",
    "csr:start": "npm run csr:build && node csrServer",
    "deploy:push": "heroku container:push web --app=battle-snakes",
    "deploy:release": "heroku container:release web --app=battle-snakes",
    "deploy": "npm run push:container && npm run release:container",
    "test": "jest",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:fix": "npm run lint:scripts:fix && npm run lint:styles:fix",
    "lint:scripts": "eslint . --ext .ts,.tsx src",
    "lint:scripts:fix": "eslint --fix . --ext .ts,.tsx src",
    "lint:styles": "stylelint \"./src/**/*.(css)\"",
    "lint:styles:fix": "stylelint \"./src/**/*.(css)\" --fix",
    "clean": "rimraf dist",
    "predev": "npm run clean",
    "dev:build:server": "cross-env NODE_ENV=development webpack --mode=development --config=./webpack/server.config.ts --watch",
    "dev:build:client": "cross-env NODE_ENV=development webpack --mode=development --config=./webpack/client.config.ts --watch",
    "dev:serve": "cross-env NODE_ENV=development nodemon --watch dist/server.js index.js",
    "dev": "npm-run-all --print-label --parallel dev:build:server dev:build:client dev:serve",
    "build:container:dev": "docker-compose -p snake-dev -f docker-compose.dev.yaml build pgadmindev",
    "build:container": "docker-compose build web",
    "build:server": "cross-env NODE_ENV=production webpack --mode=production --config=./webpack/server.config.ts",
    "build:client": "cross-env NODE_ENV=production webpack --mode=production --config=./webpack/client.config.ts",
    "build": "npm run build:server && npm run build:client",
    "serve": "cross-env NODE_ENV=production node index.js",
    "start": "npm run build && npm run serve",
    "start:container": "docker-compose --remove-orphans up web",
    "start:container:dev": "docker-compose -p snake-dev -f docker-compose.dev.yaml up pgadmindev"
  },
  "keywords": [
    "snake"
  ],
  "authors": [
    "Nikita Dronov",
    "Ivan Efimov",
    "Alexander L`vov"
  ],
  "license": "ISC",
